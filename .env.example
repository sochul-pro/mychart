# Database
DATABASE_URL="postgresql://user:password@localhost:5432/mychart"

# Auth (NextAuth v5)
AUTH_SECRET="your-secret-key-here"

# Redis (optional)
REDIS_URL="redis://localhost:6379"

# Stock Data Provider
# 'mock' (기본값) 또는 'kis' (한국투자증권)
STOCK_PROVIDER="mock"

# 한국투자증권 OpenAPI (STOCK_PROVIDER=kis 일 때 필요)
KIS_APP_KEY=""
KIS_APP_SECRET=""
KIS_ACCOUNT_NO=""
# true: 실전투자, false: 모의투자(기본값)
KIS_PRODUCTION="false"
